arch="arm64"
platform="qemu"

bootloader="u-boot"
boot_mode="extlinux"
bootargs="console=tty0 console=ttyS0,115200 rw root=/dev/vda2 rootfstype=ext4 rootwait earlycon loglevel=8"
bootloader_url="https://github.com/u-boot/u-boot"
bootloader_branch="v2025.10"
bootloader_config="qemu_arm64_defconfig"
atf_compile="no"

kernel_url="https://github.com/torvalds/linux"
kernel_branch="v6.12"
kernel_config="linux-generic-arm64-lts.config"

rootfs_arch="aarch64"

part_table="gpt"
boot_size="256"

INSTALL_U_BOOT() {
    echo "skip"
}